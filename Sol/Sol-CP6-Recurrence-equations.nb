 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{"z", "(", 
     RowBox[{"t", "+", "1"}], ")"}], "=", 
    RowBox[{
     RowBox[{"0.9", 
      RowBox[{"z", "(", "t", ")"}]}], "+", 
     RowBox[{"0.5", 
      RowBox[{"r", "(", "t", ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"r", "(", 
     RowBox[{"t", "+", "1"}], ")"}], "=", 
    RowBox[{
     RowBox[{"0.1", 
      RowBox[{"z", "(", "t", ")"}]}], "+", 
     RowBox[{"0.5", 
      RowBox[{"r", "(", "t", ")"}]}]}]}], TraditionalForm]}],ExpressionUUID->
  "da692753-c36d-4992-84b1-2904cd57295a"]
}], "Text",
