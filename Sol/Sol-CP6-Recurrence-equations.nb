Cell[CellGroupData[{

Cell["Solving recurrence equations with Mathematica", "Section",
 CellChangeTimes->{
  3.681190874062151*^9, {3.681283827281685*^9, 3.6812838409853363`*^9}, {
   3.6826665023493905`*^9, 
   3.6826665201618786`*^9}},ExpressionUUID->"a2367f21-e157-41e6-b69d-\
5eda4e77eae0"],

Cell[TextData[{
 "In Mathematica, the command RSolveValue[] is used for solving (systems of) \
recurrence equations. However, you will find not all systems can be solved \
this way; in such cases, you need to apply the method outlined in the manual \
and lectures, and solve the equations manually. Of course you can still use \
Mathematica, ",
 StyleBox["e.g",
  FontSlant->"Italic"],
 ". to calculate derivatives and to plot the solutions............ Also check \
out the command RecurrenceTable[] that enables you to iterate a first- or \
second-order recurrence equation, or a system of recurrence equations."
}], "Text",
 CellChangeTimes->{{3.681190906782179*^9, 3.681190926923607*^9}, {
   3.682666461864944*^9, 3.6826664947868524`*^9}, {3.6826665267869067`*^9, 
   3.6826666788965244`*^9}, {3.6826667147559776`*^9, 3.6826668554592743`*^9}, 
   3.6826669503935366`*^9},ExpressionUUID->"989c9353-4e09-4932-b99c-\
ceb38b0cd5ef"],

Cell[CellGroupData[{

Cell["Exercise 6-1", "Subsection",
 CellChangeTimes->{{3.682700336059181*^9, 3.6827003408184075`*^9}, {
  3.682746097450487*^9, 
  3.6827460978254676`*^9}},ExpressionUUID->"8611a58c-6e4f-4a89-83db-\
3ee45d43769b"],

Cell[TextData[{
 "Consider the second-order recurrence equations ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "(", 
     RowBox[{"t", "+", "2"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"z", "(", 
       RowBox[{"t", "+", "1"}], ")"}]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"z", "(", "t", ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "ebcb2e19-cf79-4547-a096-d3b04bf7dcb0"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "(", 
     RowBox[{"t", "+", "2"}], ")"}], "=", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"z", "(", 
       RowBox[{"t", "+", "1"}], ")"}]}], "-", 
     RowBox[{"2", 
      RowBox[{"z", "(", "t", ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "1600c4e4-c15e-44e8-b241-0c7e15947d07"],
 ". "
}], "Text",
 CellChangeTimes->{{3.6827003611416492`*^9, 3.682700526997419*^9}, 
   3.6827451058574247`*^9, {3.682745218683837*^9, 3.6827452399816055`*^9}, 
   3.682745280567659*^9, {3.6827454954446974`*^9, 
   3.6827455023773475`*^9}},ExpressionUUID->"5b8fcf5f-61af-4df9-946f-\
6fb76710af72"],

Cell[TextData[{
 "(a) Solve both equations for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "(", "0", ")"}], "=", "1"}], TraditionalForm]],
  ExpressionUUID->"44ec30d3-75f6-460e-8dfc-627f639e1b17"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "z", Cell[
    "(1)=2.5",ExpressionUUID->"b6e87087-9e2c-4151-bbff-7f9d6944aea7"]}], 
   TraditionalForm]],ExpressionUUID->"d20be98d-1bcf-4ae1-9e3a-4322e7164cbc"]
}], "Text",
 CellChangeTimes->{{3.682745263404422*^9, 
  3.6827453010372915`*^9}},ExpressionUUID->"0b41eb67-96b1-41a1-985b-\
ba0ea6298dbd"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"eq1", "=", 
  RowBox[{
   RowBox[{"z", "[", 
    RowBox[{"t", "+", "2"}], "]"}], "\[Equal]", 
   RowBox[{
    RowBox[{"0.5", "*", 
     RowBox[{"z", "[", 
      RowBox[{"t", "+", "1"}], "]"}]}], "+", 
    RowBox[{"0.5", "*", 
     RowBox[{"z", "[", "t", "]"}]}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{"solEQ1", "=", 
  RowBox[{"RSolveValue", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"eq1", ",", 
      RowBox[{
       RowBox[{"z", "[", "0", "]"}], "\[Equal]", "1"}], ",", 
      RowBox[{
       RowBox[{"z", "[", "1", "]"}], "\[Equal]", "2.5"}]}], "}"}], ",", 
    RowBox[{"z", "[", "t", "]"}], ",", "t"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.682700549742284*^9, 3.682700722846923*^9}, {
  3.6827453100063763`*^9, 3.6827453280540185`*^9}, {3.682745739919834*^9, 
  3.6827457412792664`*^9}},ExpressionUUID->"25dd30c9-df14-4522-8ba0-\
8c05e379466f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"z", "[", 
   RowBox[{"2", "+", "t"}], "]"}], "\[Equal]", 
  RowBox[{
   RowBox[{"0.5`", " ", 
    RowBox[{"z", "[", "t", "]"}]}], "+", 
   RowBox[{"0.5`", " ", 
    RowBox[{"z", "[", 
     RowBox[{"1", "+", "t"}], "]"}]}]}]}]], "Output",
 CellChangeTimes->{{3.682700708190143*^9, 3.682700724096245*^9}, 
   3.6827458972858067`*^9},ExpressionUUID->"56f0f02c-0ae1-4f99-9ecb-\
e101ad2b6828"],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", "1.`"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"-", "2.`"}], "+", 
    RowBox[{"1.`", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "0.5`"}], ")"}], "t"]}]}], ")"}]}]], "Output",
 CellChangeTimes->{{3.682700708190143*^9, 3.682700724096245*^9}, 
