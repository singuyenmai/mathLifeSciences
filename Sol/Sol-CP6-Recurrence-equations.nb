Cell[TextData[{
 "Consider two systems of recurrence equations: \n(1)   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     RowBox[{"t", "+", "1"}], ")"}], "=", 
    RowBox[{
     RowBox[{"3", 
      RowBox[{"x", "(", "t", ")"}]}], "-", 
     RowBox[{"2", 
      RowBox[{"y", "(", "t", ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "ca81aa3a-7848-4134-b55a-a820a1ecc29e"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", 
     RowBox[{"t", "+", "1"}], ")"}], "=", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"x", "(", "t", ")"}]}], "-", 
     RowBox[{"2", 
      RowBox[{"y", "(", "t", ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "bd039080-5cf3-458c-a751-49621fcabe07"],
 "\n(2)   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     RowBox[{"t", "+", "1"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"x", "(", "t", ")"}]}], "-", 
     RowBox[{"y", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "ad11688d-8299-4a4a-af87-87155fbd1e62"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
